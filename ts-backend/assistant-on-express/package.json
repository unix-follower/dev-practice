{
  "name": "assistant-on-express",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=22.14.0"
  },
  "scripts": {
    "build": "tsc -b",
    "prod": "node ./build/server.js",
    "start": "tsc -b && npm run prod",
    "start-local": "tsc -b && NODE_ENV=local npm run prod",
    "format": "prettier --log-level warn --write '**/*.{js,cjs,jsx,ts,tsx,json,css,html}'",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^5.0.0",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "eslint-import-resolver-typescript": "^4.2.1",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
